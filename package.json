{
  "name": "react-physics",
  "version": "1.0.0",
  "description": "Elements of physics represented on web using React",
  "author": "Nitin Tulswani",
  "main": "build.js",
  "module": "module-build.js",
  "license": "MIT",
  "dependencies": {
    "@reach/router": "^1.1.1",
    "babel-plugin-emotion": "^9.2.0",
    "emotion": "^9.2.3",
    "hex-rgba": "^1.0.2",
    "p5": "^0.6.1",
    "react": "^16.4.1",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.1",
    "react-responsive-modal": "^3.0.3",
    "react-tooltip": "^3.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "parcel-bundler": "^1.9.1",
    "prettier": "^1.13.4",
    "react-scripts": "^1.1.4",
    "rollup": "^0.61.0",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "scripts": {
    "start": "parcel ./public/index.html",
    "precommit": "lint-staged",
    "rollup": "rollup -c",
    "build": "react-scripts build",
    "reset": "rm -rf .cache && rm -rf dist && yarn start",
    "flow": "flow",
    "format": "find src -name '*.js' | xargs ./node_modules/.bin/prettier --write --no-semi --single-quote"
  },
  "lint-staged": {
    "*.{js,json}": [
      "./node_modules/.bin/prettier --write --no-semi --single-quote",
      "git add"
    ]
  }
}
