{
	"name": "react-physics",
	"version": "1.0.0",
	"description": "Elements of physics represented on web using React",
	"main": "index.js",
	"author": "Nitin Tulswani",
	"license": "MIT",
	"dependencies": {
		"p5": "^0.6.1",
		"react": "^16.4.0",
		"react-dom": "^16.4.0"
	},
	"devDependencies": {
		"babel-core": "^6.26.3",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"husky": "^0.14.3",
		"lint-staged": "^7.1.3",
		"parcel-bundler": "^1.8.1",
		"prettier": "^1.13.4"
	},
	"scripts": {
		"start": "parcel ./index.html",
		"format": "find src -name '*.js' | xargs ./node_modules/.bin/prettier --write --no-semi --single-quote"
	},
	"lint-staged": {
		"*.{js,json}": [
			"./node_modules/.bin/prettier --write --no-semi --single-quote",
			"git add"
		]
	}
}
